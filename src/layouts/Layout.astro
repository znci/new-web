---
import DocumentShell from "./DocumentShell.astro";
import "../styles/main.css";

const { title } = Astro.props;
---

<DocumentShell {title}>
  <Fragment slot="head">
    <script
      src="https://kit.fontawesome.com/efda69f858.js"
      crossorigin="anonymous"
      is:inline></script>
    <script src="https://challenges.cloudflare.com/turnstile/v0/api.js" defer></script>
  </Fragment>

  <slot />
</DocumentShell>
